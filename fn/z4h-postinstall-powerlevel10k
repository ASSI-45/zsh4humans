emulate -L zsh

if [[ -e $Z4H/cache/z4h-updating ]]; then
  print -ru2 -- ${(%):-"%F{3}z4h%f: updating %Bgitstatus%b binary"}
else
  print -ru2 -- ${(%):-"%F{3}z4h%f: fetching %Bgitstatus%b binary"}
fi
GITSTATUS_CACHE_DIR=$GITSTATUS_CACHE_DIR ./gitstatus/install -f || return
zf_mkdir -p -- $Z4H/cache/powerlevel10k/p10k-root
